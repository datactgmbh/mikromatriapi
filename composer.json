{
  "name": "mikrotik/matrix-api",
  "description": "MikroTik Product Matrix API Service",
  "type": "project",
  "license": "AGPL-3.0",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.3",
    "ext-ctype": "*",
    "doctrine/orm": "^2.16",
    "symfony/console": "7.1.*",
    "symfony/framework-bundle": "7.1.*",
    "symfony/monolog-bundle": "^3.10",
    "symfony/http-client": "^7.1",
    "doctrine/doctrine-bundle": "^2.15",
    "doctrine/doctrine-migrations-bundle": "^3.4",
    "symfony/string": "7.1.*",
    "symfony/yaml": "7.1.*"
  },
  "require-dev": {
    "icanhazstring/composer-unused": "^0.9.4",
    "phpstan/extension-installer": "^1.3",
    "phpstan/phpstan": "^1.11",
    "phpstan/phpstan-doctrine": "^1.5",
    "phpstan/phpstan-symfony": "^1.4",
    "phpunit/phpunit": "^11.3",
    "symfony/browser-kit": "^7.1",
    "symfony/debug-bundle": "^7.3",
    "symfony/dotenv": "^7.1",
    "symfony/http-client": "^7.1",
    "symfony/test-pack": "^1.0",
    "symfony/twig-bundle": "^7.3"
  },
  "scripts": {
    "test": "php -d zend.assertions=1 -d assert.exception=1 vendor/bin/phpunit -c phpunit.xml.dist --testdox",
    "stan": "vendor/bin/phpstan analyse -c phpstan.neon.dist --no-progress"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  }
}